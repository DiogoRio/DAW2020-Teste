extends layout

block content
  header.w3-container.w3-pale-blue
    h1 Classes de Nível 1
  .w3-container
    ul
      each clas in classes
        li
          adress
            a(href=`/classe/c${clas.codigo}`) #{clas.codigo}
          span  - #{clas.titulo}      
  footer.w3-container.w3-pale-blue.w3-padding-medium
      address.w3-padding-16
        a(href=`/`) Inicio
        a(href="javascript:history.back()").w3-right Voltar atrás